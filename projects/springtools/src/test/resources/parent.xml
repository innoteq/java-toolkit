<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
    <beans>
        <bean class="java.lang.String" id="parent" >
            <constructor-arg value="parent" />
        </bean>

        <bean class="java.lang.String" id="parent_only" >
            <constructor-arg value="parent_only" />
        </bean>

        <bean class="java.lang.String" id="parent_imported" >
            <constructor-arg value="parent_imported" />
        </bean>
    </beans>
</beans>
